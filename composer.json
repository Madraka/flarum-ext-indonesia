{
  "name": "fransallen/flarum-ext-indonesia",
  "type": "flarum-extension",
  "description": "Indonesia language pack.",
  "keywords": ["locale"],
  "license": "MIT",
  "authors": [
    {
      "name": "Frans Allen",
      "email": "fa@marsble.com"
    }
  ],
  "support": {
    "issues": "https://github.com/fransallen/flarum-ext-indonesia/issues",
    "source": "https://github.com/fransallen/flarum-ext-indonesia"
  },
  "require": {
    "flarum/core": "^0.1.0-beta.6"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    },
    "flarum-extension": {
      "title": "Indonesia",
      "icon": {
        "image": "icon.svg",
        "backgroundColor": "#00247d",
        "backgroundSize": "cover",
        "backgroundPosition": "center"
      }
    },
    "flarum-locale": {
      "code": "id",
      "title": "Indonesia"
    }
  }
}
